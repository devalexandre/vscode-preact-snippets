{
  "name": "vscode-preact-snippets",
  "displayName": "vscode-preact-snippets",
  "description": "Code snippets for Preact",
  "version": "1.0.1",
  "keywords": [
    "preact",
    "snippets",
    "vscode"
  ],
  "author": {
    "name": "Nick Taylor",
    "email": "nick@iamdeveloper.com",
    "url": "https://iamdeveloper.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nickytonline/vscode-preact-snippets"
  },
  "scripts": {
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.json": [
      "prettier --write",
      "git add"
    ],
    "*.md": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 80
  },
  "publisher": "nickytonline",
  "engines": {
    "vscode": "^1.26.0"
  },
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/snippets.json"
      },
      {
        "language": "javascriptreact",
        "path": "./snippets/snippets.json"
      }
    ]
  },
  "devDependencies": {
    "husky": "0.14.3",
    "lint-staged": "7.2.0",
    "prettier": "1.14.2"
  },
  "license": "MIT"
}
